div.au-month-picker-panel
  au-year-picker-content(
    v-model="value",
    ref="yearContent",
    v-show="type == 'year'",
    :startDate="startDate",
    :endDate="endDate",
    :disabledDate="disabledDate"
   )
  au-month-picker-content(
    v-model="value",
    ref="monthContent",
    v-show="type == 'month'",
    :startDate="startDate",
    :endDate="endDate",
    :disabledDate="disabledDate"
  )